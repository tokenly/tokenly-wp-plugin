{
	"name": "tokenly/tokenly-wp-plugin",
	"require": {
		"phpunit/phpunit": "^7.0",
		"tokenly/hmac-auth": "1.0-dev",
		"tokenly/api-client": "1.0-dev",
		"tokenly/tokenpass-client": "1.0-dev",
		"php-di/php-di": "^6.3",
		"doctrine/annotations": "^1.13",
		"twig/twig": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"Tokenly\\Wp\\": "app/"
		}
	},
	"repositories": [
		{
			"type": "path",
			"url": "libs/tokenly/hmac-auth"
		},
		{
			"type": "path",
			"url": "libs/tokenly/api-client"
		},
		{
			"type": "path",
			"url": "libs/tokenly/tokenpass-client"
		}
	],
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.6",
		"wp-coding-standards/wpcs": "^2.3",
		"phpcompatibility/phpcompatibility-wp": "*",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7"
	}
}
